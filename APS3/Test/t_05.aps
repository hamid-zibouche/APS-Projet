[
  CONST tab (vec int) (alloc 5);
  VAR i int;

  PROC REC fill [var i: int] [
    IF (lt i 5) [
      SET (nth tab i) (mul i 10);
      SET i (add i 1);
      CALL fill (adr i)
    ] [
      ECHO -1
    ]
  ];

  SET i 0;
  CALL fill (adr i);

  ECHO (nth tab 0);
  ECHO (nth tab 1);
  ECHO (nth tab 2);
  ECHO (nth tab 3);
  ECHO (nth tab 4)
]
